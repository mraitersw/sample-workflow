{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0", 
    "parameters": {
        "accounts_Cognitive_PVA_Region": {
            "defaultValue": "westus",
            "type": "String"
        }
    },   
    "variables": {
        "StorageAccountName":"samplewebworflow"
    },
    "resources": [
        {
            "type": "Microsoft.CognitiveServices/accounts",
            "apiVersion": "2021-10-01",
            "name": "samplewebworflow",
            "location": "[parameters('accounts_Cognitive_PVA_Region')]",
            "sku": {
                "name": "S0"
            },
            "kind": "CognitiveServices",
            "identity": {
                "type": "None",
                "userAssignedIdentities": {}
            },
            "properties": {
                "apiProperties": {},
                "customSubDomainName": "samplewebworflow",
                "publicNetworkAccess": "Enabled",
                "storageAccountConnectionString": "AccountName=samplewebworflow"
            }
            
        }
    ],
    "outputs": {
        "rkkeyvault": {
          "value": "[Concat('DefaultEndpointsProtocol=https;AccountName=',variables('StorageAccountName'),';AccountKey=',listKeys(resourceId('Microsoft.Storage/storageAccounts', variables('StorageAccountName')), providers('Microsoft.Storage', 'storageAccounts').apiVersions[0]).keys[0].value)]",  
          "type": "string"
        }
      }   
}